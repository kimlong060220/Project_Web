rt);\r\n  const { cartItems, error } = cart;\r\n  const dispatch = useDispatch();\r\n  useEffect(() => {\r\n    if (productId) {\r\n      dispatch(addToCart(productId, qty));\r\n    }\r\n  }, [dispatch, productId, qty]);\r\n\r\n  const removeFromCartHandler = (id) => {\r\n    dispatch(removeFromCart(id));\r\n  };\r\n  console.log(cart)\r\n  const checkpayment = () => {\r\n    props.history.push('/signin?redirect=placeorder');\r\n  };\r\n  return (\r\n    <>\r\n    <div className=\"row top\">\r\n      <div className=\"col-1\">\r\n        <h1>Shopping Cart</h1>\r\n        {cartItems.length === 0 ? (\r\n          <MessageBox>\r\n            Cart is empty. <Link to=\"/\">Go Shopping</Link>\r\n          </MessageBox>\r\n        ) : (\r\n          <ul>\r\n            {cartItems.map((item) => (\r\n              <li key={item.product}>\r\n                <div className=\"row\">\r\n                  <div>\r\n                    <img\r\n                      src={item.image}\r\n                      alt={item.name}\r\n                      className=\"small\"\r\n                    ></img>\r\n                  </div>\r\n                  <div className=\"min-30\">\r\n                    <Link to={`/product/${item.product}`}>{item.name}</Link>\r\n                  </div>\r\n                  <div>\r\n                    <select\r\n                      value={item.qty}\r\n                      onChange={(e) =>\r\n                        dispatch(\r\n                          addToCart(item.product, Number(e.target.value))\r\n                        )\r\n                      }\r\n                    >\r\n                      {[...Array(item.countInStock).keys()].map((x) => (\r\n                        <option key={x + 1} value={x + 1}>\r\n                          {x + 1}\r\n                        </option>\r\n                      ))}\r\n                    </select>\r\n                  </div>\r\n                  <div>${item.price * item.qty}</div>\r\n                  <div>\r\n                    <button\r\n                      type=\"button\"\r\n                      onClick={() => removeFromCartHandler(item.product)}\r\n                    >\r\n                      Delete\r\n                    </button>\r\n                  </div>\r\n                </div>\r\n              </li>\r\n            ))}\r\n          </ul>\r\n        )}\r\n        <PaymentMethodScreen></PaymentMethodScreen>\r\n      </div>\r\n      <div className=\"col-1\">\r\n        <ShippingAddressScreen history={props.history}></ShippingAddressScreen>\r\n      </div>\r\n    </div>\r\n    <div className=\"col-1\">\r\n    <div className=\"card card-body\">\r\n      <ul>\r\n        <li>\r\n          <h2>\r\n            Tổng Tiền ({cartItems.reduce((a, c) => a + c.qty, 0)} items) : $\r\n            {cartItems.reduce((a, c) => a + c.price * c.qty, 0)}\r\n          </h2>\r\n        </li>\r\n        <li>\r\n          <button\r\n            type=\"button\"\r\n            onClick={checkpayment}\r\n            className=\"primary block\"\r\n            disabled={cartItems.length === 0}\r\n          >\r\n            Thanh Toán\r\n          </button>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n  </>\r\n  );\r\n}\r\n","C:\\Users\\Long\\Documents\\GitHub\\Project_Web\\frontend\\src\\screens\\PlaceOrderScreen.js",["181"],"import React, { useEffect } from 'react';\r\nimport { useDispatch, useSelector } from 'react-redux';\r\nimport { Link } from 'react-router-dom';\r\nimport { createOrder } from '../actions/orderActions';\r\nimport MessageBox from '../components/MessageBox';\r\n\r\nexport default function PlaceOrderScreen(props) {\r\n  const cart = useSelector((state) => state.cart);\r\n  \r\n  // if (!cart.paymentMethod) {\r\n  //   props.history.push('/payment');\r\n  // }\r\n  const orderCreate = useSelector((state) => state.orderCreate);\r\n  const { loading, success, error, order } = orderCreate;\r\n  const toPrice = (num) => Number(num.toFixed(0)); // 5.12